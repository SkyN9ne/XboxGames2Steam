{
  "manifest_version": 2,
  "name": "included-with-xbox-game-pass",
  "version": "0.3.0",
  "description": "A browser extension that adds the mention \"included / not included with Xbox Game Pass\" on Steam's store page.",
  "permissions": [
    "storage",
    "https://gabinaureche.com/included-with-xbox-game-pass/games.json"
  ],
  "content_scripts": [
    {
      "matches": ["*://store.steampowered.com/app/*"],
      "js": ["content.ts"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  }
}
